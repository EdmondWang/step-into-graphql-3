{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": [false],
        "variable-name": [false],
        "adjacent-overload-signatures": [false],
        "trailing-comma": [ false ],
        "no-console": false,
        "quotemark": [true, "single", "jsx-double"],
        "max-line-length": [true, 120]
    },
    "rulesDirectory": []
}